{
  "name": "bdd-wallet",
  "version": "1.0.0",
  "description": "Behavior Driven Developement school project to use cucumber as a testing plateform",
  "main": "./build/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc && npm run clean-build",
    "clean-build": "rimraf ./build/features && mv ./build/src/* ./build && rmdir ./build/src",
    "test:cucumber": "cucumber-js -p default",
    "test:unit": "npm run build && mocha ./test/test.js"
  },
  "keywords": [
    "gherkin",
    "cucumber",
    "cucumber-js",
    "typescript"
  ],
  "author": "Maxime d'Harboull√©",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0",
    "@types/chai": "^4.2.14",
    "@types/cucumber": "^7.0.0",
    "@types/node": "^12.18.2",
    "chai": "^4.2.0",
    "cucumber-pretty": "^6.0.0",
    "cucumber-tsflow": "^4.0.0-rc.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0"
  }
}
